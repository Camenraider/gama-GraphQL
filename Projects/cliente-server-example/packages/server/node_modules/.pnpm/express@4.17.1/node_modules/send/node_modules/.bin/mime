#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/d/GitHub/gama-GrafhQL/Projects/cliente-server-example/packages/server/node_modules/.pnpm/mime@1.6.0/node_modules/mime/node_modules:/mnt/d/GitHub/gama-GrafhQL/Projects/cliente-server-example/packages/server/node_modules/.pnpm/mime@1.6.0/node_modules:/mnt/d/GitHub/gama-GrafhQL/Projects/cliente-server-example/packages/server/node_modules/.pnpm/node_modules:/mnt/d/GitHub/gama-GrafhQL/Projects/cliente-server-example/packages/server/node_modules:/mnt/d/GitHub/gama-GrafhQL/Projects/cliente-server-example/packages/node_modules:/mnt/d/GitHub/gama-GrafhQL/Projects/cliente-server-example/node_modules:/mnt/d/GitHub/gama-GrafhQL/Projects/node_modules:/mnt/d/GitHub/gama-GrafhQL/node_modules:/mnt/d/GitHub/node_modules:/mnt/d/node_modules:/mnt/d/GitHub/gama-GrafhQL/Projects/cliente-server-example/packages/server/node_modules/.pnpm/send@0.17.1/node_modules/mime/node_modules:/mnt/d/GitHub/gama-GrafhQL/Projects/cliente-server-example/packages/server/node_modules/.pnpm/send@0.17.1/node_modules:/mnt/d/GitHub/gama-GrafhQL/Projects/cliente-server-example/packages/server/node_modules/.pnpm/send@0.17.1/node_modules/send/node_modules"
else
  export NODE_PATH="$NODE_PATH:/mnt/d/GitHub/gama-GrafhQL/Projects/cliente-server-example/packages/server/node_modules/.pnpm/mime@1.6.0/node_modules/mime/node_modules:/mnt/d/GitHub/gama-GrafhQL/Projects/cliente-server-example/packages/server/node_modules/.pnpm/mime@1.6.0/node_modules:/mnt/d/GitHub/gama-GrafhQL/Projects/cliente-server-example/packages/server/node_modules/.pnpm/node_modules:/mnt/d/GitHub/gama-GrafhQL/Projects/cliente-server-example/packages/server/node_modules:/mnt/d/GitHub/gama-GrafhQL/Projects/cliente-server-example/packages/node_modules:/mnt/d/GitHub/gama-GrafhQL/Projects/cliente-server-example/node_modules:/mnt/d/GitHub/gama-GrafhQL/Projects/node_modules:/mnt/d/GitHub/gama-GrafhQL/node_modules:/mnt/d/GitHub/node_modules:/mnt/d/node_modules:/mnt/d/GitHub/gama-GrafhQL/Projects/cliente-server-example/packages/server/node_modules/.pnpm/send@0.17.1/node_modules/mime/node_modules:/mnt/d/GitHub/gama-GrafhQL/Projects/cliente-server-example/packages/server/node_modules/.pnpm/send@0.17.1/node_modules:/mnt/d/GitHub/gama-GrafhQL/Projects/cliente-server-example/packages/server/node_modules/.pnpm/send@0.17.1/node_modules/send/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../mime/cli.js" "$@"
else
  exec node  "$basedir/../../../mime/cli.js" "$@"
fi
